<div class="container mt-5" id="Orçamento">

</div>


@section scripts{

    <script>
        $(document).ready(function () {


            if ("@ViewBag.éPessoa" == "True") {

                $.ajax
                ({
                    type: 'GET',
                    url: "/Orçamento/VisualizarOrçamentoPessoaPartial",
                    dataType: 'html',
                    cache: false,
                    success: function (data) {
                        $('#Orçamento').append(data);
                    }
                });
            }

            else if ("@ViewBag.éOficina" == "True") {

                $.ajax
                ({
                    type: 'GET',
                    url: "/Orçamento/VisualizarOrçamentoOficinaPartial",
                    dataType: 'html',
                    cache: false,
                    success: function (data) {
                        $('#Orçamento').append(data);
                    }
                });
            }

        });
    </script>
}